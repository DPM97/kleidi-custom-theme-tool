<html>
  <head>
    {% include './partials/style.liquid' %}
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      rel="stylesheet"
      href="./testing/assets/css/flash.min.css"
    />
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"
    />
    <link
      rel="stylesheet"
      href="./testing/assets/css/bootstrap-grid.min.css"
    />
    <script src="https://js.stripe.com/v3/"></script>
    <link
      rel="stylesheet"
      type="text/css"
      href="./testing/assets/css/main.css"
    />
        <script
    src="https://code.jquery.com/jquery-3.5.1.min.js"
    integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
    crossorigin="anonymous"
  ></script>
  <script src="./testing/assets/js/flash.min.js"></script>
  <script src="./testing/assets/js/flash.jquery.min.js"></script>
  {% if msg %}
  <script>
    let type = '{{ msg.type }}';
    let msg = '{{ msg.content }}';
    if (type == 'success') {
      window.FlashMessage.success(msg, {
        progress: true,
        interactive: true,
        timeout: 6000,
        appear_delay: 200,
        container: '.flash-container',
        theme: 'default'
      });
    } else {
      window.FlashMessage.error(msg, {
        progress: true,
        interactive: true,
        timeout: 6000,
        appear_delay: 200,
        container: '.flash-container',
        theme: 'default'
      });
    }
  </script>
  {% endif %}
  </head>
  <div class="nav" style="z-index: 1000;">
    <input type="checkbox" id="nav-check" />
    <div class="nav-header">
      {% if logoURI %}
      <img class="logo" src="{{ logoURI }}" alt="logo" />
      {% else %}
      <img
        class="logo"
        src="https://kleidi.io/assets/img/logo-white.png"
        alt="logo"
      />
      {% endif %}
    </div>
    <div class="nav-btn">
      <label for="nav-check">
        <span></span>
        <span></span>
        <span></span>
      </label>
    </div>
    <div class="nav-links">
      <a class="fade-in" href="/">Home</a>
      {% if features %}
      <a class="fade-in" href="/#features">Features</a>
      {% endif %} {% if faq %}
      <a class="fade-in" href="/#faq">Faq</a>
      {% endif %} {% if contact %}
      <a class="fade-in" href="/#contact">Contact</a>
      {% endif %}
      <a class="fade-in" href="/dashboard">Dashboard</a>
      {% if user %}
      <a class="cta fade-in login" href="/auth/logout"><button>Log Out</button></a>
      {% else %}
      <a class="cta fade-in login" href="/auth/discord"><button>Log In</button></a>
      {% endif %}
    </div>
  </div>
</html>
